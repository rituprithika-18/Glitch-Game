            source:"Video.mp4"
            state:"play"
            options:{'eos':'loop'}
        Button:
            text:"Next"
            font_name:"novem___.ttf"
            font_size:32
            background_color:'#000000'
            font_color:'#808080'
            pos_hint:{"center_x":0.9}
            size_hint:(0.3,0.15)
