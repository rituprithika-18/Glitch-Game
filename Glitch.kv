        Label:
            text:"\n\nYou are 19 and end up in an Escape Room game,\n\nComplete each room to survive...\n\nPlayers In The Room"
            text_size:self.size
            halign:"left"
            valign:"top"

            font_size:24
            font_name:"Pixeboy.ttf"

        Label:
            #size_hint_x:0.2
            text:"DR. Vikram Sinha\n\n# Smarter than you think he is"
            text_size:self.size
            halign:"left"
            valign:"top"
            font_size:24
            font_name:"Pixeboy.ttf"


        Label:
            text:"Winston Churchill\n\n# Retirement has lead him to a\n\n lot of free time, all he can think about is books\n\n"
            text_size:self.size
            halign:"left"
            valign:"top"
            font_size:24
            font_name:"Pixeboy.ttf"

        Label:
            text:"\nAlisha Oberoi\n\n# Money can buy happiness"
            text_size:self.size
            halign:"left"
            valign:"top"
            font_size:24
            font_name:"Pixeboy.ttf"

        Label:
            text:"\nEmma Hopkins\n\n# Don't judge a book buy its cover"
            text_size:self.size
            halign:"left"
            valign:"top"
            font_size:24
            font_name:"Pixeboy.ttf"

        Button:
            text:"Next"
            font_name:"Pixeboy.ttf"
            font_size:32
